{
  "version": "1.0.0-*",
  "compilationOptions": {
    "emitEntryPoint": true
  },

  "dependencies": {
    "Microsoft.AspNet.Proxy": "1.0.0-rc2-16034",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc2-16236",
    "Microsoft.Extensions.Configuration.FileProviderExtensions": "1.0.0-rc2-15952",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-15952",
    "Microsoft.ServiceFabric": "4.4.87-preview",
    "Microsoft.ServiceFabric.AspNet": "1.0.0-*",
    "Microsoft.ServiceFabric.Services": "1.4.87-preview"
  },

  "frameworks": {
    "dnx451": {
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0"
      }
    }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "PackageRoot"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc",
    "PackageRoot"
  ]
}
